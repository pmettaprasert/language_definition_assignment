/*
* Shows the if elif and else
*/ 

func main() {
    var score: Int = 85;
    if score >= 90 {
        print("A");
    } elif score >= 80 and score < 90 {
        print("B");
    } elif score >= 70 {
        print("C");
    } elif score >= 60 {
        print("D");
    } else {
        print("F");
    }
    print("\n");

    //Should print out "B"
}